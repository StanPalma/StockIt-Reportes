}

<h2>Reservas</h2>

<div class="col-lg-6">
    <label>Fecha inicio</label>
    <input class="form-control" type="date" />
    <br />
</div>

<div class="col-lg-6">
    <label>Fecha final</label>
    <input class="form-control" type="date" />
    <br />
</div>

<div class="col-lg-6">
    <div class="col-lg-3">
        <input class="btn-success" type="submit" value="Filtrar" />
    </div>
    <div class="col-lg-3">
        <input class="btn-danger" type="reset" value="Limpiar" />
    </div>
</div>

<div class="col-lg-12">
    <br />
</div>

<table class="table table-bordered">
    <tr>
        <th>Nombre cliente</th>
        <th>Fecha de reserva</th>
        <th>Fecha promesa de reserva</th>
        <th>Cantidad</th>
        <th>Total</th>
    </tr>
    <tr>
        <td><br /></td>
        <td><br /></td>
        <td><br /></td>
        <td><br /></td>
        <td><br /></td>
    </tr>
</table>

